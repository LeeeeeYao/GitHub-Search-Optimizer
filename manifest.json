{
    "manifest_version": 3,
    "name": "GitHub Search Optimizer",
    "version": "1.0",
    "description": "Optimize your GitHub search queries with AI.",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  